{
    "extends": ["react-app", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "semi": true,
                "trailingComma": "none",
                "singleQuote": false,
                "printWidth": 85,
                "tabWidth": 2,
                "useTabs": false
            }
        ]
    }
}